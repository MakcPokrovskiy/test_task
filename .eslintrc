{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb/base"],
  "rules": {
    "no-plusplus": "off",
    "max-len": ["error", 120],
    "no-tabs": "off",
    "no-console": "off",
    "no-alert": "off",
    "linebreak-style": "off",
    "strict": "off",
    "import/prefer-default-export": "off"
  }
}
